/home/devel/hcsvlab-web/shared/log/*.log {
    rotate 52
    weekly
    copytruncate
    missingok
    notifempty
    olddir /home/devel/hcsvlab-web/shared/log_archive
    compress
    dateext
}

/home/devel/hcsvlab-web/shared/log/*.output {
    rotate 52
    weekly
    copytruncate
    missingok
    notifempty
    olddir /home/devel/hcsvlab-web/shared/log_archive
    compress
    dateext
}
